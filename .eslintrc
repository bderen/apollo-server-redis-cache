{
  parser: "babel-eslint",
  extends: "eslint:recommended",
  env: {
    node: true,
    es6: true,
  },
  rules: {
    "comma-dangle": [1, "always-multiline"],
    "no-console": 0,
    "padded-blocks": ["error", "never"],
    "key-spacing": 2,
    "comma-spacing": 2,
    "space-infix-ops": 2,
  },
}
